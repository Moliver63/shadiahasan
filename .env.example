# Forge API key (backend).
BUILT_IN_FORGE_API_KEY=xxxxxxxxxxxxxxxx

# Forge API base URL (backend).
BUILT_IN_FORGE_API_URL=https://api.seudominio.com

# MySQL connection string: mysql://user:pass@host:3306/db
DATABASE_URL=mysql://USUARIO:SENHA@HOST:3306/NOME_DO_BANCO

# Default sender email address.
FROM_EMAIL=contato@shadiahasan.club

# JWT signing secret (use a long random string).
JWT_SECRET=COLOQUE_UMA_CHAVE_FORTE_DE_64+_CARACTERES

# Runtime mode: development | production
NODE_ENV=production

# OAuth server base URL.
OAUTH_SERVER_URL=https://oauth.seudominio.com

# Owner/admin OpenID (if used by your auth flow).
OWNER_OPEN_ID=xxxxxxxxxxxxxxxx

# Server port (Node/Express). In cPanel Node App, you can set this too.
PORT=3000

# Resend API key for email sending.
RESEND_API_KEY=re_xxxxxxxxxxxxxxxxx

# Public site URL (used for redirects/callbacks/links).
SITE_URL=https://www.shadiahasan.club

# Frontend app id (exposed to browser).
VITE_APP_ID=xxxxxxxxxxxxxxxx

# Frontend Forge API key (exposed to browser).
VITE_FRONTEND_FORGE_API_KEY=xxxxxxxxxxxxxxxx

# Frontend Forge API base URL (exposed to browser).
VITE_FRONTEND_FORGE_API_URL=https://api.seudominio.com

# Frontend OAuth portal URL (exposed to browser).
VITE_OAUTH_PORTAL_URL=https://oauth.seudominio.com
